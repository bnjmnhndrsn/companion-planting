<div class="row">
    <div class="col-md-12">
        <h2>Your Gardens</h2>
    </div>
</div>
<% _.each(items, function(item){ %>
    <div class="row">
        <div class="col-md-2">
            <a href="gardens/<%= item.id %>"><%= item.title || item.id %></a>
        </div>
    </div>
<% }) %>
<div class="row">
    <div class="col-md-2">
        <a class="btn btn-default" href="gardens/new">New Garden</a>
    </div>
</div>
